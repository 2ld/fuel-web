#!/usr/bin/env ruby

require 'rubygems'
require 'orchestrator'

class ConsoleReporter
  def report(msg)
    p msg
  end
end

reporter = ConsoleReporter.new

nodes = [{'mac' => 'devnailgun.mirantis.com', 'role' => 'test_controller'}]
orchestrator = Orchestrator::Orchestrator.new
orchestrator.deploy(reporter, nodes)
